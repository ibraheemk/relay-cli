{
  "name": "@relaypro/cli",
  "description": "Relay developer CLI",
  "version": "0.0.0",
  "author": "Brandon Smith @BrandonSmith",
  "bin": {
    "relay": "./bin/run"
  },
  "bugs": "https://github.com/relaypro/relay-js/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.4",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-commands": "1.3.0",
    "@oclif/plugin-help": "3.2.2",
    "@oclif/plugin-not-found": "1.2.4",
    "cli-ux": "5.5.1",
    "conf": "9.0.2",
    "debug": "4.3.1",
    "enquirer": "2.3.6",
    "express": "4.17.1",
    "form-urlencoded": "4.2.1",
    "http-call": "5.3.0",
    "lodash": "4.17.21",
    "netrc-parser": "3.1.6",
    "ngrok": "3.4.0",
    "nodemon": "2.0.7",
    "open": "7.4.2",
    "tslib": "2.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@types/chai": "~4.2.15",
    "@types/form-urlencoded": "~2.0.1",
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.31",
    "@types/uuid": "8.3.0",
    "babel-plugin-root-import": "6.6.0",
    "chai": "4.3.0",
    "eslint": "7.20.0",
    "globby": "11.0.2",
    "mocha": "8.3.0",
    "nock": "13.0.8",
    "nyc": "15.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/package-lock.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/BrandonSmith/relay",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "relay",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-commands"
    ],
    "topics": {
      "workflow": {
        "description": "manage workflow configurations"
      }
    },
    "macos": {
      "identifier": "Developer ID Application: Republic Wireless, Inc."
    }
  },
  "repository": "relaypro/relay-js",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
